{
  "$schema": "http://json-schema.org/schema#",
  "title": ".NET SDK Downloads",
  "description": "All available installation packages from https://aka.ms/dotnet-download",
  "type": "object",
  "properties": {
    "versions": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "CURRENT",
              "END_OF_LIFE",
              "LTS",
              "MAINTENANCE",
              "PREVIEW"
            ]
          },
          "endOfSupport": {
            "type": [
              "string",
              "null"
            ],
            "format": "date"
          },
          "releases": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "released": {
                  "type": "string",
                  "format": "date"
                },
                "securityFixes": {
                  "type": "boolean"
                },
                "sdks": {
                  "type": "array",
                  "minItems": 1,
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "vs": {
                        "type": "string"
                      },
                      "packages": {
                        "type": "array",
                        "minItems": 1,
                        "items": {
                          "type": "object",
                          "properties": {
                            "platform": {
                              "type": "string"
                            },
                            "architecture": {
                              "type": "string"
                            },
                            "url": {
                              "type": "string",
                              "format": "uri"
                            }
                          },
                          "required": [
                            "platform",
                            "architecture",
                            "url"
                          ]
                        }
                      }
                    },
                    "required": [
                      "name",
                      "vs",
                      "packages"
                    ]
                  }
                }
              },
              "required": [
                "name",
                "released",
                "sdks"
              ]
            }
          }
        },
        "required": [
          "name",
          "status",
          "endOfSupport",
          "releases"
        ]
      }
    }
  }
}
